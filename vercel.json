{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "github": {
    "autoAlias": true,
    "enabled": true,
    "silent": false
  },
  "crons": [
    { "path": "/api/prewarm-social-light", "schedule": "0 2 * * *" },
    { "path": "/api/prewarm-assets", "schedule": "0 3 * * *" }
  ]
}
